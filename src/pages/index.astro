---
import MainLayout from "../layouts/MainLayout.astro";
import Button from '../components/Button.astro';
import Link from '../components/Link.astro';
import { Image } from 'astro:assets';
import thumbsup from "../assets/images/thumbsup.png";
import Articlecard from "../components/Articlecard.astro";

import IconButton from "../components/Button.astro";
import Kontaktform from '../components/Kontaktform.astro';
import Forsamtale from "../components/Forsamtale.astro"

import Inputfield from '../components/Inputfield.astro';
import Overskrift from "../components/Overskrift.astro";
import TextDot from "../components/TextDot.astro";
import Textbox from "../components/Textbox.astro";
---

<MainLayout title="Stoisk Ro">
	
   
    <section
    class="grid grid-cols-grid-mobil lg:grid-cols-grid-desktop mx-margin-mobil lg:mx-margin-desktop gap-gap-mobil lg:gap-gap-desktop"
    >
    <div class="col-start-2 col-span-10">
        
    <Overskrift text="jeg kan hjælpe"></Overskrift>
  

        <div class="flex flex-col items-start gap-[16px] lg:gap-[32px] ">
        <h6
            class="font-dmsans text-h7-mobil lg:text-h7-desktop text-green-500 uppercase"
        >
            med problematikker indenfor
        </h6>
        <div class="flex flex-row justify-between lg:w-full items-start">
        <div class="flex flex-wrap gap-[8px] max-w-[179px] lg:max-w-[500px]">
        <TextDot input="depression"></TextDot>
        <TextDot input="angst"></TextDot>
        <TextDot input="ptsd"></TextDot>
        <TextDot input="parforhold"></TextDot>
        <TextDot input="traume"></TextDot>
        <TextDot input="selvvard"></TextDot>
        <TextDot input="vrede"></TextDot>
        <TextDot input="livskrise"></TextDot>
        <TextDot input="stress"></TextDot>
        <TextDot input="ledelse"></TextDot>
        </div>
    <img class="lg:hidden" src="/img/stress.svg" alt="Stress Illustration">
    <img class="hidden lg:block" src="/img/stress-d.svg" alt="Stress Illustration">

        </div>
        </div>
    </div>
    </section>




	<h1 class="text-h1-mobil uppercase lg:font-gloock font-bold text-green-500">DM sans</h1>
	<h1 class="font-gloock">Gloock</h1>
	<p class="text-p-desktop">P desktop style</p>

	<!-- Skriv her hvilken variant og størrelse du skal bruge ex. primary large -->

	<IconButton text="Send" variant="primary" size="large">
    </IconButton>

	<!-- Kontaktform sat op i forhold til grid -->

<section class="grid grid-cols-grid-mobil lg:grid-cols-grid-desktop bg-green-500">

    <div class="col-start-2 col-span-10 lg:col-start-6 lg:col-span-6">
     <Kontaktform></Kontaktform>
	</div>
</section>

	<!-- forsamtale sat op i forhold til grid -->

<section class="grid grid-cols-grid-mobil lg:grid-cols-grid-desktop bg-beige-500">

    <div class="col-start-2 col-span-10 lg:col-start-8 lg:col-span-4">
     <Forsamtale></Forsamtale>
	</div>
</section>	 



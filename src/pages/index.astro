---
import MainLayout from "../layouts/MainLayout.astro";
import Button from '../components/Button.astro';
import Link from '../components/Link.astro';
import { Image } from 'astro:assets';
import thumbsup from "../assets/images/thumbsup.png";
import IconButton from "../components/Button.astro";

---

<MainLayout title="Stoisk Ro">


	<h1 class="text-h1-mobil uppercase lg:font-gloock font-bold text-green-500">DM sans</h1>
	<h1 class="font-gloock">Gloock</h1>
	<h1 class="text-p-desktop">P desktop style</h1>

	<IconButton text="Send" variant="primary" size="small">
    </IconButton>
		

    <IconButton text="Kontakt" variant="primary-negative" size="small">
	
	</IconButton>

	<IconButton text="Læs mere" variant="secondaryLeft" size="large">

    </IconButton>

	<IconButton text="Hør mere" variant="secondary-right" size="large">
		
	</IconButton>

	<IconButton text="Læs mere" variant="secondary-negative" size="large">
	
	</IconButton>

	<IconButton text="" variant="secondary-iconOnly" size="large">
			
	</IconButton>
	
</MainLayout>


